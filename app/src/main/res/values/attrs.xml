<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitledImageButton">
        <attr name="tib_title" format="string" />
        <attr name="tib_titleStyle" format="reference" />
        <attr name="tib_titleColor" format="color" />
        <attr name="tib_icon" format="reference" />
        <attr name="tib_backgroundColor" format="color" />
        <attr name="tib_tint" format="color" />
        <attr name="tib_enabled" format="boolean" />
        <attr name="tib_orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

    </declare-styleable>
</resources>